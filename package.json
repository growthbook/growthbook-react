{
  "version": "0.2.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@growthbook/growthbook-react",
  "author": "Jeremy Dorn",
  "module": "dist/growthbook-react.esm.js",
  "size-limit": [
    {
      "path": "dist/growthbook-react.cjs.production.min.js",
      "limit": "4 KB"
    },
    {
      "path": "dist/growthbook-react.esm.js",
      "limit": "4 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@growthbook/growthbook": "^0.10.0"
  },
  "description": "Powerful A/B testing for React Apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growthbook/growthbook-react.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
